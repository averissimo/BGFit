<%= form_for([@result, @line]) do |f| %>
<table>
	<thead>
		<tr>
			<th scope="col"><%= f.label :time %></th>
			<th scope="col"><%= f.label :od600 %></th>
			<th scope="col"><%= f.label :ln_od600 %></th>
			<th scope="col"><%= f.label :ph %></th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><%= f.text_field :time %></td>
			<td><%= f.text_field :od600 %></td>
			<td><%= f.text_field :ln_od600 %></td>
			<td><%= f.text_field :ph %></td>
		</tr>
	</tbody>
</table>
<div class="actions">
	<%= f.submit %>
</div>
  

<% end %>