<h3>Parameter for <%= @dyna_model.title %> dyna model</h3>
<p>
	<%= simple_format @dyna_model.description %>
</p>

<%= semantic_form_for([@dyna_model,@param]) do |f| %>
  <%= form_errors(f) %>

  <%= f.inputs do %>
  	<%= f.input :code %>
  	<%= f.input :human_title %>
  	<%= f.input :output_only, hint: "This should only be checked if the parameter is not used as an input for the parameters estimatin" %>
  	<%= f.input :initial_condition, hint: "This should only be checked if the model is based on differential equations"%>
  	<%= f.input :bottom, as: :number %>
  	<%= f.input :top, as: :number %>
  	<%= f.input :description, as: :text %>
  <% end %>
  
  <%= f.actions do %>
  	<%= f.action :submit, :as => :button %>
  	<%= f.action :cancel, :as => :link %>	
  <% end%>
<% end %>

<div class="clear"></div>

